<div class="broadcastsField box">
  <div class="prepareblock">
    <div class="prepareFormField">
      <%= form_for @broadcast, method: :post, remote: true, class: "form-group" do |f| %>
        <%= f.text_field :url, placeholder: "ニコ生URLを入力 例)http://live.nicovideo.jp/watch/lv*******?ref=community", class: "form-control" %>
        <%= f.submit '入力', type: 'submit', class: 'btn' %>
      <% end %>
    </div>
  </div>
</div>
